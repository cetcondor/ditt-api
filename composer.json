{
    "type": "ditt-api",
    "require": {
        "php": "^7.4",
        "ext-iconv": "*",
        "api-platform/api-pack": "^1.3",
        "guzzlehttp/guzzle": "^7.2",
        "html2text/html2text": "^4.3",
        "lexik/jwt-authentication-bundle": "^2.11",
        "nelmio/cors-bundle": "^2.1",
        "sensio/framework-extra-bundle": "^6.1",
        "psr/container": "1.0.0",
        "symfony/asset": "~5.2.0",
        "symfony/console": "~5.2.0",
        "symfony/debug-pack": "^1.0",
        "symfony/expression-language": "~5.2.0",
        "symfony/filesystem": "~5.2.0",
        "symfony/finder": "~5.2.0",
        "symfony/flex": "^1.12",
        "symfony/form": "~5.2.0",
        "symfony/framework-bundle": "~5.2.0",
        "symfony/monolog-bundle": "^3.6",
        "symfony/orm-pack": "^2.1",
        "symfony/process": "~5.2.0",
        "symfony/proxy-manager-bridge": "~5.2.0",
        "symfony/runtime": "~5.3.0",
        "symfony/security-bundle": "~5.2.0",
        "symfony/serializer-pack": "^1.0",
        "symfony/swiftmailer-bundle": "^3.5",
        "symfony/validator": "~5.2.0",
        "symfony/web-link": "~5.2.0",
        "symfony/yaml": "~5.2.0",
        "welp/ical-bundle": "^1.0"
    },
    "require-dev": {
        "api-platform/schema-generator": "^3.0",
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.3",
        "codeception/module-db": "^1.1",
        "codeception/module-doctrine2": "^1.1",
        "codeception/module-phpbrowser": "^1.0",
        "codeception/module-rest": "^1.2",
        "codeception/module-symfony": "^2.0",
        "consolidation/robo": "^2.2",
        "friendsofphp/php-cs-fixer": "^2.18",
        "phpstan/phpstan": "^0.12",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/browser-kit": "^4.4",
        "symfony/css-selector": "^4.4",
        "symfony/dotenv": "~5.2.0",
        "symfony/maker-bundle": "^1.29",
        "symfony/phpunit-bridge": "~5.2.0",
        "symfony/profiler-pack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C8MMKM2KBZD81TKKNAZ5CK3X",
            "allow-contrib": false
        }
    }
}
